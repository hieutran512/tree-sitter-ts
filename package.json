{
  "name": "tree-sitter-ts",
  "version": "0.1.1",
  "description": "Pure TypeScript parser library with declarative language profiles. No WASM required.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "tsx scripts/run-jest.ts",
    "test:watch": "tsx scripts/run-jest.ts --watch"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "keywords": [
    "parser",
    "lexer",
    "syntax",
    "tree-sitter",
    "typescript",
    "highlighting",
    "rag"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hieutran512/tree-sitter-ts"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@swc/core": "^1.13.5",
    "@swc/jest": "^0.2.39",
    "@types/jest": "^29.5.14",
    "@types/node": "^25.3.0",
    "jest": "^29.7.0",
    "tsx": "^4.20.6",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vite": "^7.3.1"
  }
}